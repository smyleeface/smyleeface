{
  "security.workspace.trust.untrustedFiles": "open",
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": true,
  "eslint.debug": true,
  "window.zoomLevel": 2,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.advanced": {},
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "gitDecoration.untrackedResourceForeground": "#f97a7a",
    "gitDecoration.ignoredResourceForeground": "#99933b",
    "gitDecoration.modifiedResourceForeground": "#6cbadb",
    "gitDecoration.stageModifiedResourceForeground": "#63e777",
    "gitDecoration.addedResourceForeground": "#63e777",
    "gitDecoration.stageDeletedResourceForeground": "#63e777",
    "gitDecoration.renamedResourceForeground": "#63e777",
    "gitDecoration.conflictingResourceForeground": "#f9c47a"
  }
}
